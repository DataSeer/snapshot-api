{
  "name": "nodejs-rest-api",
  "version": "3.2.1",
  "description": "nodejs REST API to interact with GenShare",
  "main": "src/server.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node src/server.js",
    "start:dev": "NODE_ENV=development NO_DB_REFRESH=true node src/server.js",
    "manage-permissions": "node scripts/manage_permissions.js",
    "manage-users": "node scripts/manage_users.js",
    "manage-genshare": "node scripts/manage_genshare_versions.js",
    "analyze-logs": "node scripts/analyze_logs.js",
    "refresh-ds-logs": "node scripts/refresh_ds_logs.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "prepare": "husky install",
    "sync-version": "node scripts/sync_version.js",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "post-version": "git push && git push --tags",
    "release": "standard-version",
    "db:init": "node scripts/maintenance/initDB.js init",
    "db:refresh": "node scripts/maintenance/initDB.js refresh",
    "db:check": "node scripts/maintenance/initDB.js check"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.804.0",
    "archiver": "^7.0.1",
    "axios": "^1.9.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "form-data": "^4.0.0",
    "googleapis": "^144.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "on-headers": "^1.0.2",
    "papaparse": "^5.4.1",
    "readline": "^1.3.0",
    "semver": "^7.6.3",
    "sqlite3": "^5.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.0",
    "standard-version": "^9.5.0"
  }
}
